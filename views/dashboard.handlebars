<div class="mb-5 pt-3">
    <a href="/dashboard/create/"><button id="create-post" class="button is-pulled-right"
            style="background-color: royalblue; color: white;">Create Post</button></a>

    <h2 class="title pb-4">Dashboard: Your Posts</h2>
    <hr>
    {{#if blogs.length}}
</div>

{{#each resturant }}
{{> blog-info blog}}

<a href="/dashboard/edit/{{resturaunt.id}}"><button class="button mb-5 ml-5"
        style="color: black; background-color: lightblue;">Edit Post</button></a>
</div>
{{/each}}
<p> {{resturant.title}}</p>
{{else}}
<h6 class="pb-4 has-text-centered is-italic">You have no posts...</h6>

{{/if}}